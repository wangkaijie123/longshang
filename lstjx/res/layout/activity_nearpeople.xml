<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#4CC2FF"
        android:orientation="horizontal"
        android:padding="10dp" >

        <ImageView
            android:id="@+id/goback_wd"
            android:layout_width="@dimen/image_20height"
            android:layout_height="@dimen/image_20height"
            android:background="@drawable/go_back" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView2"
            android:layout_alignBottom="@+id/textView2"
            android:layout_centerHorizontal="true"
            android:text="@string/nearPeople"
            android:textColor="#fff"
            android:textSize="@dimen/textSize_15possword" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/goback_wd"
            android:layout_toRightOf="@+id/goback_wd"
            android:text="@string/mine"
            android:textColor="#fff"
            android:textSize="@dimen/textSize_15possword" />

    </RelativeLayout>

    <ListView
        android:id="@+id/listView_nearPeople"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        android:background="#fff">
    </ListView>

</LinearLayout>
