<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
<RelativeLayout 
    android:id="@+id/item_addreceipt_LlId"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dip"
    android:layout_marginTop="10dip"
    >
    <RelativeLayout 
        android:id="@+id/receipt_add_RlId1"
       android:layout_width="match_parent"
       android:layout_height="30dip"
       
        >
        <TextView 
            android:id="@+id/username_id"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="maozedong"
            android:paddingRight="80dp"
            />
        <TextView 
            android:id="@+id/userPhone_id"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="135 **** 7629"
            android:textSize="14sp"
            android:layout_toRightOf="@id/username_id"
            />
    </RelativeLayout>
     <TextView 
         android:id="@+id/receipt_address_TvId"
         android:text="北京市朝阳区大柳树143号2单元202"
         android:gravity="center|left"
         android:layout_below="@id/receipt_add_RlId1"
         android:textSize="12sp"
         android:layout_width="wrap_content"
         android:layout_height="30dp"
         android:paddingTop="5dp"
       android:paddingBottom="12dp"
         />
          <View
              android:id="@+id/receipt_address_ViewId"
            android:layout_width="fill_parent"
            android:layout_below="@id/receipt_address_TvId"
            android:layout_height="@dimen/wire_0.5height"
            android:background="#66000000"
            android:padding="10dp" />
     <LinearLayout 
         android:id="@+id/item_receipt_LlId"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_below="@id/receipt_address_ViewId"
         android:orientation="horizontal"
         >
          <RelativeLayout
            android:id="@+id/receipt_moren_RlId"
            android:layout_width="wrap_content"
            android:layout_height="30dip"
            android:background="@drawable/bg_click"
            android:layout_gravity="center_vertical"
            android:layout_weight="3"
            android:paddingTop="6dip"
             >

           <ImageView
               android:id="@+id/receipt_ImgId"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp"
                android:background="@drawable/wodewuliu" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:text="默认地址"
                android:layout_toRightOf="@id/receipt_ImgId"
                android:textSize="12sp"
                android:textColor="@color/text_black"
              />       
        </RelativeLayout>
       <LinearLayout 
          android:layout_height="wrap_content"
          android:layout_weight="2"
          android:layout_width="wrap_content"
           >
             <LinearLayout
            android:id="@+id/item_receipt_editId"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:background="@drawable/bg_click"
            android:orientation="horizontal"
            android:layout_weight="1"
             >

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_gravity="center_vertical"
                android:background="@drawable/wodewuliu" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:text="编辑"
                android:textSize="12sp"
                android:textColor="@color/text_black"
               />       
        </LinearLayout>
         <LinearLayout
            android:id="@+id/item_receipt_decelete"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:background="@drawable/bg_click"
            android:orientation="horizontal"
            android:layout_weight="1"
             >

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:background="@drawable/wodewuliu" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:text="删除"
                android:textSize="12sp"
                android:textColor="@color/text_black"
                />       
        </LinearLayout>
       </LinearLayout>
         
     
     </LinearLayout>  
         <View
              android:id="@+id/receipt_address_ViewId2"
            android:layout_width="fill_parent"
            android:layout_below="@id/item_receipt_LlId"
            android:layout_height="@dimen/wire_0.5height"
            android:background="#66000000"
            android:padding="10dp" /> 
</RelativeLayout>
</RelativeLayout>
