            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/new2"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/new2"
                android:text="群聊"
                android:textSize="15dip" />
        </RelativeLayout>
